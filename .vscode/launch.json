{
  // 1. > ./setup-python-venv.sh
  // 2. https://code.visualstudio.com/docs/python/environments
  //    > ctrl+shift+P 
  //    > Python: Select Interpreter 
  "version": "0.2.0",
  "configurations": [
    {
      "name": "GistOps Remote Push",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/gistops/src/main.py",
      "args": ["mirror", "master"],
      "env": {
        "GISTOPS_GIT_REMOTE_URL": "ok-this-is-it"
      },
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}